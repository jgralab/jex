module java/lexical/literals/BooleanLiterals
exports
  sorts
    Bool
    BoolLiteral

  context-free syntax
    rule Bool -> BoolLiteral {cons("Bool")}
    rule "true"  -> Bool {cons("True")}
    rule "false" -> Bool {cons("False")}
