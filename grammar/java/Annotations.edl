module java/Annotations
imports
  java/Types     %% PlainClassifierType
  java/Members   %% Block
  java/Literals  %% Literal

exports
  sorts
    Annotation

  context-free syntax
    rule "@" PlainClassifierType
         ("(" {(Id "=" AnnotationValue+) ","}+ ")")?
         -> Annotation

  lexical syntax
    rule Block   -> AnnotationValue
    rule Literal -> AnnotationValue

