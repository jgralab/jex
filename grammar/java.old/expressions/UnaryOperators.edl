module java/expressions/UnaryOperators
imports 
  java/expressions/Main
exports
  context-free syntax
    rule "+" Expr -> Expr
    rule "-" Expr -> Expr

    rule "++" Expr -> Expr
    rule "--" Expr -> Expr

    rule "~" Expr -> Expr
    rule "!" Expr -> Expr

    rule "(" PrimType ")" Expr -> Expr
    rule "(" RefType  ")" Expr -> Expr
